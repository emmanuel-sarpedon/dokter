generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model LibelleProfession {
  id      Int    @id @default(autoincrement())
  libelle String

  @@unique([libelle])
}

model Agreement {
  id      Int    @id @default(autoincrement())
  libelle String

  @@unique([libelle])
}

model SesamVitale {
  id      Int    @id @default(autoincrement())
  libelle String

  @@unique([libelle])
}

model Procedure {
  id      Int    @id @default(autoincrement())
  libelle String

  @@unique([libelle])
}

model City {
  id      Int    @id @default(autoincrement())
  libelle String

  @@unique([libelle])
}

model Practitioner {
  id            Int     @id @default(autoincrement())
  civility      String // civilite
  tel           String? // column_10
  agreement     String? // column_14
  sesamVitale   String? // column_16
  name          String // nom
  address       String // adresse
  profession    String? // libelle_profession
  latitude      Float // coordonnees.lat
  longitude     Float // coordonnees.lon
  city          String? // commune
  reimbursement String? // remboursement
  procedure     String? // nom_acte
}
